{
  "name": "gen-api-docs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "build:helper": "tsc src/scripts/_helper.ts --outDir dist/scripts",
    "build:convert": "tsc src/scripts/convertYaml.ts --outDir dist/scripts --module es6 --moduleResolution nodenext",
    "preconvertYaml": "npm run build:helper && npm run build:convert && node dist/scripts/_helper.js",
    "convertYaml": "node dist/scripts/convertYaml.mjs",
    "fastStart": "npm run convertYaml && npm run start",
    "fastBuildStart": "npm run build && npm run fastStart"
  },
  "keywords": [],
