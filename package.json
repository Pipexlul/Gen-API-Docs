    "@types/express": "^4.17.17",
    "@types/node": "^20.3.3",
    "@types/swagger-ui-express": "^4.1.3",
    "genversion": "^3.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "express": "^4.18.2",
    "read-js-yaml": "^1.0.3",
    "swagger-ui-express": "^4.6.3"
  }
}
